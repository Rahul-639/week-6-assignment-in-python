class Node:
    def __init__(self, val):
        self.val = val
        self.next = None
class QueueLinkedList:
    def __init__(self):
        self.front = None
        self.rear = None
    def push(self, x):
        new_node = Node(x)
        if not self.rear:
            self.front = self.rear = new_node
            return
        self.rear.next = new_node
        self.rear = new_node
    def pop(self):
        if not self.front:
            return -1
        val = self.front.val
        self.front = self.front.next
        if not self.front:
            self.rear = None
        return val
