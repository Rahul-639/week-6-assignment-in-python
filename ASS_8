from collections import deque
def firstNonRepeating(s: str) -> str:
    freq = [0] * 26
    q = deque()
    result = []
    for ch in s:
        freq[ord(ch) - ord('a')] += 1
        q.append(ch)
        while q and freq[ord(q[0]) - ord('a')] > 1:
            q.popleft()
        result.append(q[0] if q else '#')
    return "".join(result)
